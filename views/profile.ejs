    <section id="reports" data-stellar-background-ratio="2.5">
        <div class="container">
            <div class="row">
                <div class="news-info">

                    <h2><a>Review and Check Your Test Results</a> <i class="fa fa-envelope" style="color: #bd182e; font-size: 29px;"> </i></h2>
                    <% if (typeof tests !== 'undefined' && typeof results !== 'undefined') { %>
                        <h3>You have received new emails on your inbox <i class="fa fa-eye" style="color: #bd182e; font-size: 20px;"> </i></h3>

                        <audio src="audio/notify.mp3" id="audio1" controls style="display: none;"></audio>
                        <button type="button" onclick="playaudio();" class="btn btn-info" data-toggle="collapse" data-target="#demo">Show Email</button>


                        <div id="demo" class="collapse">
                            <br>
                            <p>Dear
                                <%= tests.name %>,
                            </p>
                            <p>We hope you are doing well.</p>
                            <p>sincerely we inform you that the test you have made on
                                <%= tests.date %> by
                                    <%= tests.department %> department has showed that you and the legit son are genetically
                                        <%= results.testresult %> like a biological father and a son upon the given samples of two of you that you have provided showed you and the legit son share the same short tandem repeats and they are :
                            </p>
                            <p style="background-color: rgb(232, 235, 226); word-break: break-all;">
                                <%= results.shorttandem %>
                            </p>

                            <% } else { %>
                                <h3>You have no new received emails or Reports <i class="fa fa-eye-slash" style="color: #bd182e; font-size: 20px;"> </i></h3>
                                <% }  %>
                        </div>

                </div>


            </div>
        </div>
    </section>



    <br><br><br><br><br><br><br><br>


    <section id="online" data-stellar-background-ratio="2.5">
        <div class="container">
            <div class="row">
                <div class="news-info">

                    <h2><a> Make a Quick Online DNA Test  </a> <i class="fa fa-bar-chart" style="color: #bd182e; font-size: 29px;">  </i></h2>
                    <h3>Just Upload the Files containing 13 CODIS <i class="fa fa-cloud-upload" style="color: #bd182e; font-size: 20px;"> </i></h3>


                    <form id="checkDNA-form" action="/uploaddnafile" method="POST" enctype="multipart/form-data">

                        <!-- SECTION TITLE -->
                        <br><br>

                        <div class="wow fadeInUp " data-wow-delay="0.8s" enctype="multipart/form-data">
                            <div class="col-md-6 col-sm-6">
                                <label for="dadfile">Upload Father's File</label>
                                <input type="file" class="form-control" id="fatherfile" name="fatherfile" placeholder="Upload File" required>
                            </div>

                            <div class="col-md-6 col-sm-6 ">
                                <label for="sonfile">Upload legit Son's File</label>
                                <input type="file" class="form-control" id="sonfile" name="sonfile" placeholder="Upload File" required>
                            </div>

                            <div class="col-md-12 col-sm-12 ">
                                <br>
                                <button type="submit" class="form-control" id="cf-submit">Submit Button</button>
                                <br><br>
                            </div>
                        </div>
                    </form>

                    <h3>OR</h3>

                    <br>

                    <h3>Paste the 13 CODIS for each One in its textarea <i class="fa fa-clipboard" style="color: #bd182e; font-size: 20px;"> </i></h3>

                    <form id="checkDNAtext-form" action="/pastednatext" method="POST" enctype="multipart/form-data">

                        <!-- SECTION TITLE -->
                        <div class="wow fadeInUp " data-wow-delay="0.8s">
                            <div class="col-md-6 col-sm-6 ">
                                <br>

                                <label for="text1">Paste the Father's DNA Text </label>
                                <textarea class="form-control" rows="5" id="fathertext" name="fathertext" placeholder="Paste text" required></textarea>
                            </div>

                            <div class="col-md-6 col-sm-6 ">
                                <br>
                                <label for="text2">Paste the legit Son's DNA Text </label>
                                <textarea class="form-control" rows="5" id="sontext" name="sontext" placeholder="Paste text" required></textarea>
                            </div>

                            <div class="col-md-12 col-sm-12 ">
                                <br><br>

                                <button type="submit" class="form-control" id="cf-submit">Submit Button</button>

                            </div>
                        </div>
                    </form>
                </div>
            </div>

        </div>
        </div>
    </section>


    <section id="appointment" data-stellar-background-ratio="3">
        <div class="container">
            <div class="row">
                <div class="news-info">

                    <% if (typeof logos !== 'undefined') { %>

                        <h3>Your DNA Test Report are ready <i class="fa fa-bar-chart" style="color: #bd182e; font-size: 22px;">  </i></h3>
                        <button type="button" onclick="dynamicUrl();" class="btn btn-info" data-toggle="collapse" data-target="#redemo">Show Test Result</button>

